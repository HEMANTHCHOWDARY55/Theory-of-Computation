#include <stdio.h>
#include <stdbool.h>

bool simulateDFA(int start, char *input) {
    int state = start;
    int len = strlen(input);
    for (int i = 0; i < len; i++) {
        if (state == 0) {
            if (input[i] == 'a') {
                state = 1;
            } else if (input[i] != 'b') {
                return false;
            }
        } else if (state == 1) {
            if (input[i] == 'a') {
                state = 2;
            } else if (input[i] != 'b') {
                return false;
            }
        } else if (state == 2) {
            if (input[i] == 'a') {
                state = 1;
            } else if (input[i] == 'b') {
                return true;
            } else {
                return false;
            }
        }
    }
    return false;
}

int main() {
    int start;
    char input[100];

    printf("Enter the initial state: ");
    scanf("%d", &start);

    printf("Enter the input string: ");
    scanf("%s", input);

    if (simulateDFA(start, input)) {
        printf("Accepted\n");
    } else {
        printf("Not Accepted\n");
    }

    return 0;
}
